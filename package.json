{
  "name": "graphql-kafka-subscriptions",
  "version": "0.3.0",
  "description": "Apollo graphql subscription over Kafka protocol",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ancashoria/graphql-kafka-subscriptions.git"
  },
  "keywords": [
    "graphql",
    "kafka",
    "apollo",
    "subscriptions"
  ],
  "author": "Ancas Horia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ancashoria/graphql-kafka-subscriptions/issues"
  },
  "homepage": "https://github.com/ancashoria/graphql-kafka-subscriptions#readme",
  "jest": {
    "roots": [
      "<rootDir>/src/test"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$"
  },
  "dependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/node": "^12.12.14",
    "bunyan": "1.8.12",
    "graphql-subscriptions": "^1.1.0",
    "iterall": "^1.2.2",
    "node-rdkafka": "^2.7.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  }
}
