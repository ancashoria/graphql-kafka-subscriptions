{
  "name": "graphql-kafka-subscriptions",
  "version": "0.2.6",
  "description": "Apollo graphql subscription over Kafka protocol",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ancashoria/graphql-kafka-subscriptions.git"
  },
  "keywords": [
    "graphql",
    "kafka",
    "apollo",
    "subscriptions"
  ],
  "author": "Ancas Horia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ancashoria/graphql-kafka-subscriptions/issues"
  },
  "homepage": "https://github.com/ancashoria/graphql-kafka-subscriptions#readme",
  "jest": {
    "roots": [
      "<rootDir>/src/test"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$"
  },
  "dependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/node": "^9.3.0",
    "bunyan": "1.8.12",
    "graphql-subscriptions": "^0.5.0",
    "iterall": "^1.1.1",
    "node-rdkafka": "2.7.1-2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "3.5.1"
  }
}
